<% if !@contract.new_record? %>
  <input type="hidden" name="contract_id" value="<%= @contract.id %>">
<% else %>
  <input type="hidden" name="contract[status]"            id="contract_status"            value="<%= @contract.status %>">
  <input type="hidden" name="contract[contract_type]"     id="contract_contract_type"     value="<%= @contract.contract_type %>">
  <input type="hidden" name="contract[rental_bond]"       id="contract_rental_bond"       value="<%= @contract.rental_bond %>">
  <input type="hidden" name="contract[property_postcode]" id="contract_property_postcode" value="<%= @contract.property_postcode %>">
  <input type="hidden" name="contract[start_of_lease]"    id="contract_start_of_lease"    value="<%= @contract.start_of_lease %>">
  <input type="hidden" name="contract[end_of_lease]"      id="contract_end_of_lease"      value="<%= @contract.end_of_lease %>">
  <input type="hidden" name="contract[rolling_lease]"     id="contract_rolling_lease"     value="<%= @contract.rolling_lease %>">
  <input type="hidden" name="contract[property_weekly_rent]" id="contract_property_weekly_rent" value="<%= @contract.property_weekly_rent %>">
<% end %>
